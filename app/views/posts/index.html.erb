<h1>Index- should show all posts</h1>
<% @posts.each do |post| %>
  <div class="card">
    <div class="card-body">
      <h3 class="card-title"><%= post.title %></h3>
      <p><%= post.body %></p>
      <%= link_to "Read More", post_path(post), :class => 'btn btn-secondary' %>
    </div>
  </div>
<% end %>